{
    "name": "cordova-android-sms-reader",
    "version": "0.0.1",
    "displayName": "Cordova Android SMS Reader",
    "description": "A sample Apache Cordova application that give to the sms using Android",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "declare": "tsc && rm -rf ./www/tsdist",
        "build": "(rm -r www/* || true) && parcel build ts/reader.ts -d www && npm run declare"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "author": "Jérémy Fornarino (j4yf7o)",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-android": "^8.1.0",
        "parcel-bundler": "^1.12.4"
    },
    "devDependencies": {
        "@types/cordova": "0.0.34",
        "cordova-plugin-whitelist": "^1.3.4",
        "typescript": "^3.9.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "android"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/J4YF7O/cordova-android-sms-reader.git"
    },
    "bugs": {
        "email": "j4yf7o@protonmail.com",
        "url": "https://github.com/J4YF7O/cordova-android-sms-reader/issues"
    },
    "homepage": "https://github.com/J4YF7O/cordova-android-sms-reader#readme",
    "browserslist": [
        "last 1 Chrome versions"
    ]
}
