{
  "name": "me.jeremyf.cordova.sms",
  "displayName": "Cordova Android SMS Reader",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that give to the sms using Android Inbox",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "declare": "tsc && rm -rf ./www/tsdist",
    "build": "(rm -r www/* || true) && parcel build ts/reader.ts -d www && npm run declare"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "cordova-plugin-whitelist": "^1.3.4",
    "typescript": "^3.9.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
